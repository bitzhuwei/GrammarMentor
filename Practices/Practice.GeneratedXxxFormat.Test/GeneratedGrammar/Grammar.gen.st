// 14 VnRegulations:
StatementList : StatementList Statement // [0]
              | Statement ; // [1]
Statement : SyntaxProduction // [2]
          | LexiProduction ; // [3]
SyntaxProduction : 'Vn' ':' CandidateList ';' ; // [4]
CandidateList : CandidateList '|' Candidate // [5]
              | Candidate ; // [6]
Candidate : VList // [7]
          | 'empty' ; // [8]
VList : VList V // [9]
      | V ; // [10]
V : 'Vn' // [11]
  | 'Vt' ; // [12]
LexiProduction : 'Vt' ':' 'pattern' ';' ; // [13]
// 3 VtPatterns:
'Vn' : %%[a-zA-Z_][a-zA-Z0-9_]*%% ; // [0]
'Vt' : %%'([ -&]|\\'|[(-\[]|\\\\|[\]-~])+'%% ; // [1]
'pattern' : %%[%]{2}[ -~]([^%]|%[^%])*[%]{2}%% ; // [2]
